# OpenAI API Configuration (Optional - only needed for ConciencIA feature)
# Get your API key from: https://platform.openai.com/api-keys
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Application Settings
VITE_APP_NAME=IdeasParaApp
VITE_APP_VERSION=1.0.0

# Feature Flags (Optional)
VITE_ENABLE_COMMUNITY=true
VITE_ENABLE_CONSCIENCIA=true
VITE_ENABLE_ANALYTICS=false

# Server Configuration (If using backend)
VITE_API_BASE_URL=http://localhost:3001
VITE_SERVER_TIMEOUT=30000
